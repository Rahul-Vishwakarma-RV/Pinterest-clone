<% include ./partials/header.ejs %>
<div class="w-full min-h-screen px-4 py-8">
    <div id="feed" class="cards gap-x-3.5 items-center">
        <% posts.reverse().forEach(function(elem){ %>
            <div class="card w-[15.2vw]">
                <div class=" w-full h-auto rounded-2xl overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="mt-2 text-sm"><%= elem.title %></h5>
                <h6 class="text-xs font-medium opacity-50">@<%= elem.user.username %></h6>
            </div>
       <% }) %>
    </div>
</div>
<% include ./partials/footer.ejs %>

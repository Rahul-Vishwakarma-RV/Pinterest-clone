<% include ./partials/header.ejs %>
<div class="w-full min-h-screen px-4 py-8">
    <div class="cards flex flex-wrap gap-10">
        <% user.posts.reverse().forEach(function(elem){ %>
            <div class="card w-60">
                <div class=" w-full h-auto bg-sky-200 rounded-2xl overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="mt-2 text-sm"><%= elem.title %></h5>
            </div>
       <% }) %>
    </div>
</div>
<% include ./partials/footer.ejs %>
